#include<iostream>
 
using namespace std;
int main()
{
    
    //variable declearation
    int n,T;
    //INPUT from user 
    
    cout<<"enter the value how many times you want to print : ";
    //take input
    cin>>n;
     
    //condition. this condition name is loop
    for(int i = 0; i<=n  ;i++){
        cout<<" I LOVE OUR PROPHET MOHAMMAD S.W.E"<<endl;
    }
    
   return 0;
}
