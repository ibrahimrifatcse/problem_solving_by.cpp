     #include<iostream>
 
using namespace std;
int main()
{
     
     int i=1;
     //while looping
     while(i<=1000 ){
         cout<<i<<endl;
         i++;
     }
     cout<<"-------------End of the end looping----------------";
     
   return 0;
}
     
