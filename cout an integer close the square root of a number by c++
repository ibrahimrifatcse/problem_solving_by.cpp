//cout an integer close the square root of a number


#include<iostream>
#include<cmath>
using namespace std;

int main()
{
    //variable declear
    int x,Sqrt;
    //input value
    cout<<"Enter the value  an integer number : ";
    cin>>x;
    
    Sqrt = sqrt(x);
    
    cout<<"an intger close to the squre root of a number : "<<Sqrt<<endl;
    return 0;
}
